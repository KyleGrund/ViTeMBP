<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <samplingfrequency>29.97</samplingfrequency>
    <sensornames>
        <name>Frame Accelerometer</name>
    </sensornames>
    <sensorbindings>
        <sensorbinding>
            <name>Frame Accelerometer</name>
            <binding>i2c-3:32</binding>
        </sensorbinding>
    </sensorbindings>
    <capturetype>EmbeddedH2</capturetype>
    <enablecompression>true</enablecompression>
    <cloud>
        <uploadtocloud>true</uploadtocloud>
        <deleteonuploadtocloud>true</deleteonuploadtocloud>
    </cloud>
</configuration>